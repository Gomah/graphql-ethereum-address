{
  "name": "graphql-ethereum-address",
  "description": "Ethereum Address scalar type for GraphQL",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "version": "1.0.1",
  "license": "MIT",
  "keywords": [
    "graphql",
    "scalar",
    "ethereum",
    "address",
    "gql"
  ],
  "repository": "https://github.com/Gomah/graphql-ethereum-address",
  "homepage": "https://github.com/Gomah/graphql-ethereum-address",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/graphql-ethereum-address.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "release": "yarn test && HUSKY_SKIP_HOOKS=1 standard-version && git push --follow-tags && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "size-limit": [
    {
      "path": "dist/graphql-ethereum-address.cjs.production.min.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/graphql-ethereum-address.esm.js",
      "limit": "1 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.4.0",
    "graphql": "15.8.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "standard-version": "^9.3.2",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
}
